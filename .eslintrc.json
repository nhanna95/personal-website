{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {},
  "overrides": [
    {
      "files": ["*.html"],
      "parser": null,
      "rules": {
        "no-unused-vars": "off",
        "no-undef": "off"
      }
    }
  ],
  "ignorePatterns": ["blog.html"]
}

