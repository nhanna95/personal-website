---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <!-- Homepage -->
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/</loc>
    <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
  
  <!-- Static Pages -->
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/about.html</loc>
    <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/blog.html</loc>
    <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
  </url>
  
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/contact.html</loc>
    <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.7</priority>
  </url>
  
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/resume.html</loc>
    <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.7</priority>
  </url>
  
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/meet/</loc>
    <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.6</priority>
  </url>
  
  <!-- Blog Posts -->
  {% for post in site.posts %}
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}{{ post.url }}</loc>
    <lastmod>{% if post.updated %}{{ post.updated | date: '%Y-%m-%d' }}{% else %}{{ post.date | date: '%Y-%m-%d' }}{% endif %}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  {% endfor %}
</urlset>

